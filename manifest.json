{
  "manifest_version": 2,
  "name": "Simple Graph Generatorr",
  "version": "1.0",
  "description": "Make graph of something quickly",
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",
    "clipboardRead",
    "clipboardWrite"
  ],
  "icons" : {
    "128": "icon.png"
  }
}
